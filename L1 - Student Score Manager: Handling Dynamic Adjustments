let scores = [35, 42, 88, 95, 76, 30, 99, 50, 60, 25];
let passedCount = 0; 

for (let i = 0; i < scores.length; i++) {
  if (scores[i] < 40) {
    scores[i] += 20; 
  }
  if (scores[i] > 90) {
    scores[i] = 90; 
  }
  if (scores[i] >= 50) {
    passedCount++; 
  }
}

console.log("Final Adjusted Scores:", scores);
console.log("Number of students who passed:", passedCount);
